public class Ware {
  private int id;
  private String name;
  private double preis;
  
  public Ware(int nr,String na, double pr) {
    this.id=nr;
    this.name=na;
    this.preis=pr;
  }
  
  public int gibNummer(){
    int rueckgabe=id;
    return rueckgabe;    
  }
  
  public String gibName(){
    String rueckgabe=name;
    return rueckgabe;    
  }
  
  public double gibPreis(){
    double rueckgabe=preis;
    return rueckgabe;
  }

}

